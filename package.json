{
  "name": "react-vue-plain-ssg",
  "version": "1.0.0",
  "description": "Static site generator that using React to generate pages from partials and Vue to create UI.",
  "repository": {
    "url": "flatdragon/react-vue-plain-ssg"
  },
  "license": "MIT",
  "author": "Kochetygov Andrei",
  "keywords": [
    "react",
    "vue",
    "ssg"
  ],
  "scripts": {
    "development": "mix watch --hot",
    "production": "mix --production && react-snap && prettier --write dist",
    "serve": "node serve"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "preserveLineBreaks": true,
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-beta.0",
    "@vue/compiler-sfc": "^3.0.11",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "finalhandler": "^1.1.2",
    "html-webpack-plugin": "^5.3.1",
    "laravel-mix": "^6.0.19",
    "laravel-mix-clean": "^0.1.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "react-snap": "^1.23.0",
    "serve-static": "^1.14.1",
    "vue": "^3.0.11",
    "vue-loader": "^16.1.0"
  }
}
